{
  "name": "secureflow-automaton",
  "version": "4.1.0",
  "description": "Enterprise-grade DevSecOps pipeline automation platform with comprehensive multi-tool security integration (SonarQube, Snyk, Container Security)",
  "private": true,
  "type": "module",
  "author": "TechTyphoon",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/TechTyphoon/secure-flow-automaton.git"
  },
  "keywords": [
    "devsecops",
    "security",
    "pipeline",
    "automation",
    "vulnerability",
    "monitoring",
    "react",
    "typescript",
    "sonarqube",
    "snyk",
    "container-security",
    "security-dashboard",
    "sast",
    "dependency-scanning",
    "real-time-alerts",
    "compliance-scoring"
  ],
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "os": [
    "darwin",
    "linux",
    "win32"
  ],
  "scripts": {
    "dev": "vite --host localhost --port 8080",
    "dev:windows": "set NODE_OPTIONS=--max-old-space-size=4096 && vite --host 0.0.0.0 --port 8080",
    "dev:unix": "NODE_OPTIONS=--max-old-space-size=4096 vite --host 0.0.0.0 --port 8080",
    "build": "vite build",
    "build:dev": "vite build --mode development",
    "build:prod": "vite build --mode production",
    "preview": "vite preview --host 0.0.0.0 --port 8080",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "lint": "eslint . --ext .ts,.tsx --report-unused-disable-directives --max-warnings 200 --ignore-pattern 'coverage/**' --ignore-pattern '*.backup.*'",
    "lint:fix": "eslint . --ext .ts,.tsx --fix",
    "lint:security": "npx eslint . --ext .ts,.tsx --config .eslintrc.security.cjs --format json --output-file eslint-security-results.json || true",
    "security:scan": "npm audit --audit-level=moderate --json > security-audit.json && npx eslint . --ext .ts,.tsx --config .eslintrc.security.cjs --format json --output-file eslint-security.json || true",
    "analyze": "npx vite-bundle-analyzer dist/stats.html",
    "type-check": "tsc --noEmit",
    "format": "prettier --write \"apps/web/**/*.{ts,tsx,js,jsx,json,md}\"",
    "format:check": "prettier --check \"apps/web/**/*.{ts,tsx,js,jsx,json,md}\"",
    "clean": "rm -rf dist node_modules/.vite && (rmdir /s /q dist node_modules\\.vite 2>nul || true)",
    "clean:windows": "if exist dist rmdir /s /q dist && if exist node_modules\\.vite rmdir /s /q node_modules\\.vite",
    "clean:unix": "rm -rf dist node_modules/.vite",
    "postinstall": "node tools/scripts/post-install-verify.js",
    "prestart": "npm run setup:env",
    "setup:env": "node -e \"const fs=require('fs'); if(!fs.existsSync('.env') && fs.existsSync('.env.example')) fs.copyFileSync('.env.example', '.env'); console.log('Environment setup complete');\"",
    "setup:full": "npm run clean && npm install && npm run setup:env && npm run type-check",
    "setup:ultimate": "bash ultimate-setup.sh",
    "setup:docker": "bash docker-setup.sh",
    "verify": "node tools/scripts/post-install-verify.js",
    "start": "npm run dev",
    "start:prod": "npm run build:prod && npm run preview",
    "docker:build": "docker build -f Dockerfile.ultimate -t secureflow-automaton:latest .",
    "docker:dev": "docker-compose -f docker-compose.ultimate.yml up --build secureflow-dev",
    "docker:prod": "docker-compose -f docker-compose.ultimate.yml up --build secureflow-prod",
    "docker:demo": "docker-compose -f docker-compose.demo.yml up --build -d",
    "docker:stop": "docker-compose -f docker-compose.ultimate.yml down",
    "docker:logs": "docker-compose -f docker-compose.ultimate.yml logs -f",
    "docker:clean": "docker system prune -f && docker volume prune -f"
  },
  "dependencies": {
    "@hookform/resolvers": "^3.9.0",
    "@octokit/rest": "^20.1.2",
    "@radix-ui/react-accordion": "^1.2.0",
    "@radix-ui/react-alert-dialog": "^1.1.1",
    "@radix-ui/react-aspect-ratio": "^1.1.0",
    "@radix-ui/react-avatar": "^1.1.0",
    "@radix-ui/react-checkbox": "^1.1.1",
    "@radix-ui/react-collapsible": "^1.1.0",
    "@radix-ui/react-context-menu": "^2.2.1",
    "@radix-ui/react-dialog": "^1.1.2",
    "@radix-ui/react-dropdown-menu": "^2.1.1",
    "@radix-ui/react-hover-card": "^1.1.1",
    "@radix-ui/react-label": "^2.1.0",
    "@radix-ui/react-menubar": "^1.1.1",
    "@radix-ui/react-navigation-menu": "^1.2.0",
    "@radix-ui/react-popover": "^1.1.1",
    "@radix-ui/react-progress": "^1.1.0",
    "@radix-ui/react-radio-group": "^1.2.0",
    "@radix-ui/react-scroll-area": "^1.1.0",
    "@radix-ui/react-select": "^2.1.1",
    "@radix-ui/react-separator": "^1.1.0",
    "@radix-ui/react-slider": "^1.2.0",
    "@radix-ui/react-slot": "^1.1.0",
    "@radix-ui/react-switch": "^1.1.0",
    "@radix-ui/react-tabs": "^1.1.0",
    "@radix-ui/react-toast": "^1.2.1",
    "@radix-ui/react-toggle": "^1.1.0",
    "@radix-ui/react-toggle-group": "^1.1.0",
    "@radix-ui/react-tooltip": "^1.1.4",
    "@slack/web-api": "^7.9.3",
    "@supabase/supabase-js": "^2.50.3",
    "@tanstack/react-query": "^5.56.2",
    "@types/react": "^18.0.0",
    "axios": "^1.11.0",
    "bcryptjs": "^2.4.3",
    "caniuse-lite": "^1.0.30001727",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cmdk": "^1.0.0",
    "date-fns": "^3.6.0",
    "dotenv": "^17.2.1",
    "embla-carousel-react": "^8.3.0",
    "express-rate-limit": "^7.5.1",
    "helmet": "^7.2.0",
    "input-otp": "^1.2.4",
    "jsonwebtoken": "^9.0.2",
    "lucide-react": "^0.462.0",
    "next-themes": "^0.3.0",
    "node-cron": "^3.0.3",
    "node-fetch": "^3.3.2",
    "react": "^18.3.1",
    "react-day-picker": "^8.10.1",
    "react-dom": "^18.3.1",
    "react-hook-form": "^7.53.0",
    "react-resizable-panels": "^2.1.3",
    "react-router-dom": "^6.26.2",
    "recharts": "^2.12.7",
    "sharp": "^0.32.6",
    "sonner": "^1.5.0",
    "tailwind-merge": "^2.5.2",
    "tailwindcss-animate": "^1.0.7",
    "uuid": "^9.0.1",
    "vaul": "^0.9.3",
    "winston": "^3.17.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@eslint/js": "^9.9.0",
    "@jest/globals": "^30.0.5",
    "@tailwindcss/typography": "^0.5.15",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^14.3.1",
    "@testing-library/user-event": "^14.6.1",
    "@types/bcryptjs": "^2.4.6",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^22.16.5",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@types/uuid": "^9.0.8",
    "@vitejs/plugin-react-swc": "^3.7.1",
    "@vitest/coverage-v8": "^3.2.4",
    "audit-ci": "^7.1.0",
    "autoprefixer": "^10.4.21",
    "commitizen": "^4.3.1",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^9.9.0",
    "eslint-plugin-react-hooks": "^5.1.0-rc.0",
    "eslint-plugin-react-refresh": "^0.4.9",
    "eslint-plugin-security": "^3.0.1",
    "globals": "^15.9.0",
    "husky": "^8.0.3",
    "jsdom": "^24.1.3",
    "lint-staged": "^15.5.2",
    "postcss": "^8.5.6",
    "prettier": "^3.6.2",
    "serve": "^14.2.4",
    "tailwindcss": "^3.4.11",
    "typescript": "^5.5.3",
    "typescript-eslint": "^8.0.1",
    "vite": "^6.0.0",
    "vite-bundle-analyzer": "^1.1.0",
    "vitest": "^3.2.4"
  }
}
