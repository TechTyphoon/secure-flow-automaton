{
  "name": "secureflow-automaton",
  "version": "1.0.0",
  "description": "Enterprise-grade DevSecOps pipeline automation platform with real-time security monitoring",
  "private": true,
  "type": "module",
  "author": "TechTyphoon",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/TechTyphoon/secure-flow-automaton.git"
  },
  "keywords": [
    "devsecops",
    "security",
    "pipeline",
    "automation",
    "vulnerability",
    "monitoring",
    "react",
    "typescript"
  ],
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "scripts": {
    "dev": "vite --host 0.0.0.0 --port 8080",
    "build": "vite build",
    "build:dev": "vite build --mode development",
    "build:prod": "vite build --mode production",
    "deploy": "npm run build && gh-pages -d dist",
    "deploy:vercel": "vercel --prod",
    "deploy:netlify": "netlify deploy --prod --dir=dist",
    "deploy:docker": "docker-compose -f docker-compose.prod.yml up -d --build",
    "lint": "eslint . --ext .ts,.tsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint . --ext .ts,.tsx --fix",
    "lint:security": "eslint . --config .eslintrc.security.js",
    "type-check": "tsc --noEmit",
    "preview": "vite preview --host 0.0.0.0 --port 8080",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "db:generate": "supabase gen types typescript --local > src/integrations/supabase/types.ts",
    "db:reset": "supabase db reset --local",
    "db:migrate": "supabase db push --local",
    "db:seed": "echo 'Production database seeding disabled' && exit 0",
    "security:scan": "npm audit && npm run lint:security && npm run security:dependency-check",
    "security:dependency-check": "npx audit-ci --config audit-ci.json",
    "security:container-scan": "docker run --rm -v /var/run/docker.sock:/var/run/docker.sock -v $PWD:/app aquasec/trivy image secureflow-automaton:latest",
    "security:pipeline": "node scripts/production-security-pipeline.js",
    "jenkins:validate": "cd jenkins && jenkins-plugin-cli --plugins plugins.txt --dry-run",
    "jenkins:install-plugins": "cd jenkins && jenkins-plugin-cli --plugins plugins.txt",
    "jenkins:config-check": "cd jenkins && jenkins-casc-check --config jenkins.yaml",
    "jenkins:start": "docker-compose -f jenkins/docker-compose.jenkins.yml up -d",
    "jenkins:stop": "docker-compose -f jenkins/docker-compose.jenkins.yml down",
    "jenkins:logs": "docker-compose -f jenkins/docker-compose.jenkins.yml logs -f",
    "docker:build": "docker build -t secureflow-automaton .",
    "docker:build:dev": "docker build -f Dockerfile.dev -t secureflow-automaton:dev .",
    "docker:run": "docker run -p 8080:80 secureflow-automaton",
    "docker:dev": "docker-compose -f docker-compose.dev.yml up --build",
    "docker:prod": "docker-compose -f docker-compose.prod.yml up --build",
    "docker:stop": "docker-compose down && docker-compose -f docker-compose.dev.yml down && docker-compose -f docker-compose.prod.yml down",
    "docker:clean": "docker system prune -f && docker volume prune -f",
    "start": "npm run dev",
    "start:prod": "npm run build:prod && npm run preview"
  },
  "dependencies": {
    "@hookform/resolvers": "^3.9.0",
    "@octokit/rest": "^20.1.2",
    "@radix-ui/react-accordion": "^1.2.0",
    "@radix-ui/react-alert-dialog": "^1.1.1",
    "@radix-ui/react-aspect-ratio": "^1.1.0",
    "@radix-ui/react-avatar": "^1.1.0",
    "@radix-ui/react-checkbox": "^1.1.1",
    "@radix-ui/react-collapsible": "^1.1.0",
    "@radix-ui/react-context-menu": "^2.2.1",
    "@radix-ui/react-dialog": "^1.1.2",
    "@radix-ui/react-dropdown-menu": "^2.1.1",
    "@radix-ui/react-hover-card": "^1.1.1",
    "@radix-ui/react-label": "^2.1.0",
    "@radix-ui/react-menubar": "^1.1.1",
    "@radix-ui/react-navigation-menu": "^1.2.0",
    "@radix-ui/react-popover": "^1.1.1",
    "@radix-ui/react-progress": "^1.1.0",
    "@radix-ui/react-radio-group": "^1.2.0",
    "@radix-ui/react-scroll-area": "^1.1.0",
    "@radix-ui/react-select": "^2.1.1",
    "@radix-ui/react-separator": "^1.1.0",
    "@radix-ui/react-slider": "^1.2.0",
    "@radix-ui/react-slot": "^1.1.0",
    "@radix-ui/react-switch": "^1.1.0",
    "@radix-ui/react-tabs": "^1.1.0",
    "@radix-ui/react-toast": "^1.2.1",
    "@radix-ui/react-toggle": "^1.1.0",
    "@radix-ui/react-toggle-group": "^1.1.0",
    "@radix-ui/react-tooltip": "^1.1.4",
    "@slack/web-api": "^7.9.3",
    "@supabase/supabase-js": "^2.50.3",
    "@tanstack/react-query": "^5.56.2",
    "@types/react": "^18.0.0",
    "axios": "^1.7.7",
    "bcryptjs": "^2.4.3",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cmdk": "^1.0.0",
    "date-fns": "^3.6.0",
    "dotenv": "^17.0.1",
    "embla-carousel-react": "^8.3.0",
    "express-rate-limit": "^7.5.1",
    "helmet": "^7.2.0",
    "input-otp": "^1.2.4",
    "jsonwebtoken": "^9.0.2",
    "lucide-react": "^0.462.0",
    "next-themes": "^0.3.0",
    "node-cron": "^3.0.3",
    "react": "^18.3.1",
    "react-day-picker": "^8.10.1",
    "react-dom": "^18.3.1",
    "react-hook-form": "^7.53.0",
    "react-resizable-panels": "^2.1.3",
    "react-router-dom": "^6.26.2",
    "recharts": "^2.12.7",
    "sharp": "^0.32.6",
    "sonner": "^1.5.0",
    "tailwind-merge": "^2.5.2",
    "tailwindcss-animate": "^1.0.7",
    "uuid": "^9.0.1",
    "vaul": "^0.9.3",
    "winston": "^3.17.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@eslint/js": "^9.9.0",
    "@tailwindcss/typography": "^0.5.15",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^14.3.1",
    "@testing-library/user-event": "^14.6.1",
    "@types/bcryptjs": "^2.4.6",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^22.5.5",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@types/uuid": "^9.0.8",
    "@vitejs/plugin-react-swc": "^3.5.0",
    "@vitest/coverage-v8": "^1.6.1",
    "audit-ci": "^7.1.0",
    "autoprefixer": "^10.4.20",
    "commitizen": "^4.3.1",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^9.9.0",
    "eslint-plugin-react-hooks": "^5.1.0-rc.0",
    "eslint-plugin-react-refresh": "^0.4.9",
    "eslint-plugin-security": "^3.0.1",
    "globals": "^15.9.0",
    "husky": "^8.0.3",
    "jsdom": "^24.1.3",
    "lint-staged": "^15.5.2",
    "lovable-tagger": "^1.1.7",
    "postcss": "^8.4.47",
    "tailwindcss": "^3.4.11",
    "typescript": "^5.5.3",
    "typescript-eslint": "^8.0.1",
    "vite": "^5.4.1",
    "vitest": "^1.6.1"
  }
}
